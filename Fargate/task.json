{
    "family": "SteveZou-Basic",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "example-app",
            "image": "docker.io/jacksondavenport/random-logger:latest"
        },
        {
            "name": "datadog-agent",
            "image": "public.ecr.aws/datadog/agent:latest",
            "essential": true,
            "environment": [
                {
                    "name": "DD_API_KEY",
                    "value": "<MY_API_KEY>"
                },
                {
                    "name": "ECS_FARGATE",
                    "value": "true"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  